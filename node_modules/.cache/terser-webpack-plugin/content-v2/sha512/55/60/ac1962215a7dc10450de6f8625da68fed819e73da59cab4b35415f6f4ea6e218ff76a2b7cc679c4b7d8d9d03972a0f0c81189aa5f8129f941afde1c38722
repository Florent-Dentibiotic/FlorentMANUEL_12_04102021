/** MOUSE OVER ANIMATIONS */\n\n        sessions.map((session, index) => {\n            const animations = svg.append('g')\n            animations\n                .append('circle')\n                .attr('id', 'circle' + index)\n                .attr('cx', function (d) {\n                    return x(session.day)\n                })\n                .attr('cy', function (d) {\n                    return y(session.sessionLength)\n                })\n                .attr('r', 4)\n                .attr('fill', 'white')\n                .attr('transform', 'translate(0, 100)')\n                .attr('opacity', '0')\n            animations\n                .append('rect')\n                .attr('id', 'infos' + index)\n                .attr('height', 25)\n                .attr('width', 39)\n                .attr('fill', '#fff')\n                .attr('x', function (d) {\n                    return x(session.day)\n                })\n                .attr('y', function (d) {\n                    return y(session.sessionLength)\n                })\n                .attr('transform', 'translate(0, 70)')\n                .attr('opacity', 0)\n            animations\n                .append('text')\n                .attr('id', 'time' + index)\n                .text(session.sessionLength + ' min')\n                .attr('font-size', '8')\n                .attr('fill', 'black')\n                .attr('x', function (d) {\n                    return x(session.day)\n                })\n                .attr('y', function (d) {\n                    return y(session.sessionLength)\n                })\n                .attr('transform', 'translate(7, 85)')\n                .attr('opacity', 0)\n            animations\n                .append('rect')\n                .attr('height', 250)\n                .attr('width', 250 - index * 41.5)\n                .attr('fill', '#000000')\n                .attr('x', function (d) {\n                    return index * 41.5\n                })\n   